{
  "name": "nuxt-ghost",
  "version": "1.0.0",
  "description": "My self directed study, exploring Nuxt.js and Ghost",
  "author": "dylan-chew",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
