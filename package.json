    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.15.3",
    "prismjs": "^1.23.0",
    "vue-kinesis": "^1.2.3",
    "vue-prism-component": "^1.2.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.1.3",
    "ava": "^3.15.0",
